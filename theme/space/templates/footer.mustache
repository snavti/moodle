{{!
    Page footer.
}}

<footer id="s-page-footer" class="wrapper-page" {{#footerbgimg}}style="background-image: url('{{{footerbgimg}}}');"{{/footerbgimg}} {{#footercustomcss}}{{{.}}}{{/footercustomcss}}>
    <div class="{{^isfrontpage}}wrapper-fw{{/isfrontpage}} {{#isfrontpage}}wrapper-xl{{/isfrontpage}}">

        {{#footerblock1}}
        <div class="rui-footer-content rui-footer-block--1">
            <div class="mb-5">
            {{{ footerblock1 }}}
            </div>
        </div>
        <hr class="rui-footer-hr--1" />
        {{/footerblock1}}

        {{#footerblock2}}
        <div class="rui-footer-content rui-footer-block--2">
            {{{ footerblock2 }}}
        </div>
        <hr class="rui-footer-hr--2" />
        {{/footerblock2}}

        {{#footerblock3}}
