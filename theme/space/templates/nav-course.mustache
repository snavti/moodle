{{!
   Custom Course Nav
}}
{{#showcoursenav}}
<div class="rui-course-nav-container">
    <div class="header-links">
        {{#headerlinksdata}}
            {{#status}}
            <a href="{{{url}}}" title="{{{title}}}" id="{{itemid}}" class="header-link-item {{#isactiveitem}}active{{/isactiveitem}}">
                <span class="header-link-item-icon">{{{icon}}}</span>
                {{{title}}}
            </a>
            {{/status}}
        {{/headerlinksdata}}
    </div>
 </div>
{{/showcoursenav}}